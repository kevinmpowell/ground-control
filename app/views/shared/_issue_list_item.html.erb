<li class="issue-list-item <%= issue.closed? ? 'closed' : 'open' %> <%= issue.client_name %>" data-issue-id="<%= issue.id %>">
	<h2 class="issue-list-item-title"><a href="<%= issue.html_url %>"><%= issue["title"] %></a></h2>
	<span class="issue-list-item-number">#<%= issue["number"] %></span>
	<p><%#= raw @markdown_renderer.render(issue["body"]) %></p>
	<span><%= issue.repo_name %></span>
</li>